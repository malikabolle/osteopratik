{
  "name": "osteopratik",
  "version": "1.0.3",
  "license": "All right resserved",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --aot --build-optimizer",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "app-cache": "node ./scripts/app-cache.js",
    "debug-ios": "remotedebug_ios_webkit_adapter",
    "service-worker": "sw-precache --root=dist --config=./scripts/sw-precache-config.js",
    "deploy-dev": "ng build --aot --dev && npm run service-worker && npm run app-cache && firebase deploy",
    "deploy": "npm run build --prod && npm run service-worker && npm run app-cache && firebase deploy",
    "deploy-patch": "npm version patch && npm run build --prod && npm run service-worker && npm run app-cache && firebase deploy",
    "deploy-minor": "npm version minor && npm run build --prod && npm run service-worker && npm run app-cache && firebase deploy",
    "deploy-major": "npm version major && npm run build --prod && npm run service-worker && npm run app-cache && firebase deploy",
    "serve-prod": "npm run build --prod && npm run service-worker && npm run app-cache && live-server ./dist"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.0",
